---
import { Container, FeatureCard } from '@littlesticks/odyssey-theme-components';
import ProfileCard from '../../components/cards/ProfileCard.astro';
import ImageCard from '../../components/cards/ImageCard.astro';
import {Icon} from 'astro-icon'
import {
  AnnouncementBar,
	Button,
	CustomerQuoteSection,
	ThreeColumnTextSection,
	YouTubeEmbedSection,
  StickyTextImageSection,
  CtaCardSection
} from '@littlesticks/odyssey-theme-components';
import Layout from '../../layouts/Page.astro';
import TextAndImageHero from '../../components/sections/heros/TextAndImageHero.astro';

const seo = {
	title: 'Makerspaces in Jamaica | Tech To Empower',
	description: 'Landing Page #1 | Astro Odyssey Theme',
};

---

<Layout seo={seo} >
	<TextAndImageHero reversed>
		<div slot="text-container">
			<h1>Makerspaces in Jamaica</h1>
			<p>We are currently coordinating with the PDP of Jamaica and multiple Jamaican FTC teams to send 4 3d printers to the country. This initiative is estimated to give over 50 teams access to 3d printers.</p>
		</div>
		</div>
		<picture slot="image-container">
			<img
				src="/assets/images/home/screenshots/landing-2.png"
				alt="Astro Odyssee"
				id="home-hero-image"
			/>
		</picture>
	</TextAndImageHero>
	<Container>
		<section class="feature-card__section" id="officiers">
			<h2>Pictures</h2>
			<div class="feature-card__grid">
				{<ImageCard imgSrc={`/assets/images/home/screenshots/jamaica/1`} desc="Meeting with the Jamaica PDP"></ImageCard>}
				
			</div> 
		</section>
	</Container>
</Layout>

<style>
	.feature-card__section {
		margin: var(--section-margin) auto;
	}
	.feature-card__grid {
		display: grid;
		grid-template-columns: repeat(2, 1fr);
		gap: var(--theme-grid-gap);
	}
	@media (max-width: 768px) {
		.feature-card__grid {
			grid-template-columns: 1fr;
		}
	}
	.asdsa {
		min-height: 100px;

	}
</style>

