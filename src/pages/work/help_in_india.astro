---
import {Icon} from 'astro-icon'
import {
  AnnouncementBar,
	Button,
	CustomerQuoteSection,
	ThreeColumnTextSection,
	YouTubeEmbedSection,
  StickyTextImageSection,
  CtaCardSection
} from '@littlesticks/odyssey-theme-components';
import Layout from '../../layouts/Page.astro';
import TextAndImageHero from '../../components/sections/heros/TextAndImageHero.astro';

const seo = {
	title: 'Help In India| Tech To Empower',
	description: 'Landing Page #1 | Astro Odyssey Theme',
};
---

<Layout seo={seo} >
	<TextAndImageHero reversed>
		<div slot="text-container">
			<h1>STEM in India</h1>
			<p>We partnered with an organization called Village Tech Schools to promote FIRST in rural parts of India to give kids oppurtinities in STEM. As of now, we have made 5 FTC (FIRST Tech Challege) teams, and provided the teams with 3D printers, starter kits, and other parts.</p>
		</div>
		</div>
		<picture slot="image-container">
			<img
				src="/assets/images/home/screenshots/landing-1.png"
				alt="Astro Odyssee"
				id="home-hero-image"
			/>
		</picture>
	</TextAndImageHero>
</Layout>

